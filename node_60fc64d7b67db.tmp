function menorNumDivisivelPorTodos(){
    let menorNum = 0;
    var state = true;
    while (state) {
        let qtdNaoDivisores = 0
        let numTestado = 1
        for (var x = 1; x <= 10; x++) {            
            console.log(numTestado)
            if (numTestado % x !== 0) {
                qtdNaoDivisores++
            }
            
            if (qtdNaoDivisores > 0) {
                menorNum++
                break
            } else if (x == 21 && qtdNaoDivisores == 0) {
                state = false
                return menorNum
            }
        }
    }
}
console.log(menorNumDivisivelPorTodos())