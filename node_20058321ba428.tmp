function menorNumDivisivelPorTodos(){
    var state = true
    let menorNum = 1
    while (state) {
        for(var x = 1; x <= 20; x++) {
            var localizador = 10000
            if (menorNum > localizador) {
                console.log(menorNum)
                localizador += 10000
            }
            if (menorNum % x != 0) {
                menorNum++
                break;
            } else if (x == 20) {
                state = false
                return menorNum
            }
        }
    }
}
console.log(menorNumDivisivelPorTodos())