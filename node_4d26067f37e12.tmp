function menorNumDivisivelPorTodos(){
    var state = true
    let menorNum = 1
    while (state) {
        for(var x = 1; x <= 10; x++) {
            console.log(menorNum)
            if (menorNum % x != 0) {
                menorNum++
                break;
            } else if (x == 21) {
                state = false
                return menorNum
            }
        }
    }
}
console.log(menorNumDivisivelPorTodos())